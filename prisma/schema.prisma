
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User{
  id       String @id @default(auto()) @map("_id") @db.ObjectId  
  name     String
  email    String @unique
  password String
  phoneNumber Int
  isAdmin Boolean? @default(false)
  address String? @default("")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}